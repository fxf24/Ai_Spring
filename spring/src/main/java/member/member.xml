<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="dao" class="member.MemberDAO">
	<property name="memberVO" ref="vo2"></property>
</bean>

<bean id="vo" class="member.MemberVO">
<constructor-arg value="spring"></constructor-arg>
<constructor-arg value="work"></constructor-arg>
</bean>

<bean id="vo2" class="member.MemberVO">
<property name="id" value="servlet"></property>
<property name="pw" value="jsp"></property>

</bean>
</beans>
